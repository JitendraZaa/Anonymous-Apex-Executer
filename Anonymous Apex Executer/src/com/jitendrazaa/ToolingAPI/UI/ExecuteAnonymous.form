        <Component class="javax.swing.JTextArea" name="txtCode">
          <Properties>
            <Property name="columns" type="int" value="20"/>
            <Property name="rows" type="int" value="5"/>
            <Property name="text" type="java.lang.String" value="List&lt;PSS_Product_Selected__c&gt; lstProductSelected = [SELECT  PSS_Opportunity__c, &#xa;                                                    PSS_Opportunity__r.Rate_Only_renewal__c , &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;PSS_Opportunity__r.PSUID_Text__c ,                                                                                                                                                                                                   &#xa;                                                    TPID__c FROM PSS_Product_Selected__c &#xa;                                                    WHERE TPID__c != null  &#xa;                                                    AND &#xa;                                                    PSS_Opportunity__r.Rate_Only_renewal__c = false&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;PSS_Opportunity__r.PSUID_Text__c != null&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;PSS_Opportunity__r.Developer_Note__c = null&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;];&#xa;System.debug(&apos;** Count **&apos;+lstProductSelected.size());&#xa;&#xa;//PSS Opp - Key, TPID - Value&#xa;Map&lt;Id,Set&lt;String&gt;&gt; mpPSSOpp = new Map&lt;Id,Set&lt;String&gt;&gt;();&#xa;&#xa;//PSS Opp - Key , PSUID - Value&#xa;Map&lt;Id,Decimal&gt; mpPSSOpp_PSUID = new Map&lt;Id,Decimal&gt;();&#xa;&#xa;&#xa;&#xa;&#xa;for(PSS_Product_Selected__c p : lstProductSelected)&#xa;{&#xa;&#x9;Set&lt;String&gt; setTPIDOnOpp = mpPSSOpp.get(p.PSS_Opportunity__c);&#xa;&#x9;&#xa;&#x9;if(setTPIDOnOpp == null)&#xa;&#x9;&#x9;setTPIDOnOpp = new Set&lt;String&gt;();&#xa;&#x9;&#xa;&#x9;setTPIDOnOpp.add(p.TPID__c);               &#xa;&#x9;mpPSSOpp.put(p.PSS_Opportunity__c, setTPIDOnOpp) ;&#xa;&#x9;mpPSSOpp_PSUID.put(p.PSS_Opportunity__c, Decimal.valueOf( p.PSS_Opportunity__r.PSUID_Text__c) );&#xa;}&#xa;&#xa;//COmplete only 1 PSS Opp at a time and then Break&#xa;for(String pssOppId : mpPSSOpp.keyset())&#xa;{&#xa;&#x9;Set&lt;String&gt; setTPIDOnOpp = mpPSSOpp.get(pssOppId) ;&#xa;&#x9;Set&lt;String&gt; activeTPIDSet = new set&lt;String&gt;() ; &#xa;&#x9;&#xa;&#x9;List&lt;PSS_Plan_Summary__c&gt; p = [Select &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Plan_Id__c &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;PSS_Plan_Summary__c &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Plan_Id__c IN :setTPIDOnOpp &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Status__c != &apos;CANCEL&apos; &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Status__c != &apos;PURGE&apos;&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Status__c != &apos;DELETE&apos;&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND &#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;PSUID__c = :mpPSSOpp_PSUID.get(pssOppId)] ;&#xa;&#x9;for(PSS_Plan_Summary__c pObj : p)&#xa;&#x9;{&#xa;&#x9;&#x9;activeTPIDSet.add(pObj.Plan_Id__c);&#xa;&#x9;}&#xa;&#x9;if(setTPIDOnOpp.size() == activeTPIDSet.size())&#xa;&#x9;{&#xa;&#x9;&#x9;PSS_opportunity__c psstoUpdate = new PSS_opportunity__c(Id=pssOppId, Rate_Only_renewal__c = true, Developer_Note__c = &apos;Processed by Renewal Script&apos;);&#xa;&#x9;&#x9;update psstoUpdate ;&#xa;&#x9;&#x9;System.debug(&apos;*** Its Rate Only Renewal and record being updated is ***&apos;+pssOppId);&#xa;&#x9;&#x9;System.debug(&apos;*** TPID in PSS Opp ***&apos;+setTPIDOnOpp.size());&#xa;&#x9;&#x9;System.debug(&apos;*** Active TPID ***&apos;+activeTPIDSet.size());&#xa;&#x9;&#x9;&#x9;&#x9;&#xa;&#x9;}else{ &#xa;&#x9;&#x9;PSS_opportunity__c psstoUpdate = new PSS_opportunity__c(Id=pssOppId, Developer_Note__c = &apos;Processed by Renewal Script&apos; );&#xa;&#x9;&#x9;update psstoUpdate ;&#xa;&#x9;&#x9;&#xa;&#x9;&#x9;System.debug(&apos;*** NOT Rate Only Renewal and record being updated is ***&apos;+pssOppId);&#xa;&#x9;&#x9;System.debug(&apos;*** TPID in PSS Opp ***&apos;+setTPIDOnOpp.size());&#xa;&#x9;&#x9;System.debug(&apos;*** Active TPID ***&apos;+activeTPIDSet.size());&#xa;&#x9;}&#xa;&#x9;break; &#xa;}&#xa;System.debug(&apos;** PSS Opp Count **&apos;+mpPSSOpp.keyset().size());&#xa;"/>
          </Properties>
        </Component>
